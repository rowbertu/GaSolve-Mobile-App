{
  "expo": {
    "name": "GaSolve",
    "slug": "LPGSafetyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ICON.png",
    "scheme": "lpgsafetyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/RED_LOGO.png"
    },
    "splash": {
      "image": "./assets/images/ICON.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/ICON.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.LPGSafetyApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/RED_LOGO.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/RED_LOGO.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7d3d5e76-8019-4f7c-a749-d5c15cbdcfb7"
      }
    },
    "owner": "rowbertu"
  }
}
